<section class="story story-1">
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-md-offset-2 col-sm-10 col-sm-offset-1 text-center">
                <h2>@Strings.StoryTitle</h2>
                <h6>@Strings.StoryDetails</h6>
            </div>
        </div>

        <div class="row">
            <div class="col-md-3 col-md-offset-2 col-sm-6 feature">
                <img alt="Photo" src="~/images/tall2.jpg">
                <p>@Strings.StoryAboutHer</p>
            </div>


            <div class="col-md-3 col-md-offset-2 col-sm-6 feature">
                <img alt="Photo" src="~/images/tall1.jpg">
                <p>@Strings.StoryAboutHim</p>
            </div>


        </div>
    </div>
</section>

@Html.Partial("_section_separator")

<section class="story story-1">
    <div class="container">

        <div class="row">
            <div class="col-md-8 col-md-offset-2 col-sm-10 col-sm-offset-1 text-center feature">
                <h2>@Strings.OurStoryTitle</h2>
                <h6>@Strings.OurStoryDetails</h6>
            </div>
        </div>

        <div class="row">
            <ul class="timeline">
                <li class="timeline-item">
                    <div class="timeline-badge"><img src="~/Images/timeline/start.png" /></div>
                    <div class="timeline-panel">
                        <div class="timeline-heading">
                            <h4 class="timeline-title">@Strings.OurStoryHowWeMet</h4>
                            <p><small class="text-muted"><i class="glyphicon glyphicon-calendar"></i> @Strings.OurStoryHowWeMetDate</small></p>
                        </div>
                        <div class="timeline-body">
                            <p>@Strings.OurStoryHowWeMetDetails1</p>
                            <p>@Strings.OurStoryHowWeMetDetails2</p>
                        </div>
                    </div>
                </li>
                <li class="timeline-item">
                    <div class="timeline-badge"><img src="~/Images/timeline/first_date.png" /></div>
                    <div class="timeline-panel">
                        <div class="timeline-heading">
                            <h4 class="timeline-title">@Strings.OurStoryOurFirstDate</h4>
                            <p><small class="text-muted"><i class="glyphicon glyphicon-calendar"></i> @Strings.OurStoryOurFirstDateDate</small></p>
                        </div>
                        <div class="timeline-body">
                            <p>@Strings.OurStoryOurFirstDateDetails1</p>
                            <p>@Strings.OurStoryOurFirstDateDetails2</p>
                        </div>
                    </div>
                </li>
                <li class="timeline-item">
                    <div class="timeline-badge"><img src="~/Images/timeline/travel.png" /></div>
                    <div class="timeline-panel">
                        <div class="timeline-heading">
                            <h4 class="timeline-title">@Strings.OurStoryOurFirstHoliday</h4>
                            <p><small class="text-muted"><i class="glyphicon glyphicon-calendar"></i> @Strings.OurStoryOurFirstHolidayDate</small></p>
                        </div>
                        <div class="timeline-body">
                            <p>@Strings.OurStoryOurFirstHolidayDetails1</p>
                            <p>@Strings.OurStoryOurFirstHolidayDetails2</p>
                        </div>
                    </div>
                </li>

                <li class="timeline-item">
                    <div class="timeline-badge"><img src="~/Images/timeline/engagement-ring.png" /></div>
                    <div class="timeline-panel">
                        <div class="timeline-heading">
                            <h4 class="timeline-title">@Strings.OurStoryHowHeProposed</h4>
                            <p><small class="text-muted"><i class="glyphicon glyphicon-calendar"></i> @Strings.OurStoryHowHeProposedDate</small></p>
                        </div>
                        <div class="timeline-body">
                            <p>@Strings.OurStoryHowHeProposedDetails1</p>
                            <p>@Strings.OurStoryHowHeProposedDetails2</p>
                        </div>
                    </div>
                </li>

                <li class="timeline-item">
                    <div class="timeline-badge"><img src="~/Images/timeline/new_chapter.png" /></div>
                    <div class="timeline-panel">
                        <div class="timeline-heading">
                            <h4 class="timeline-title">@Strings.OurStoryNewChapter</h4>
                            <p><small class="text-muted"><i class="glyphicon glyphicon-calendar"></i>  @Strings.OurStoryNewChapterDate</small></p>
                        </div>
                        <div class="timeline-body">
                            <p>@Strings.OurStoryNewChapterDetails1</p>
                            <p>@Strings.OurStoryNewChapterDetails2</p>
                        </div>
                    </div>
                </li>

            </ul>
        </div>
    </div>
</section>
