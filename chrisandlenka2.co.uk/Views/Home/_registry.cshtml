<section class="registry registry-1 registry-3 overlay-light parallax">

    <div class="background-image-holder">
        <img alt="Background image" class="background-image" src="~/images/hero6.jpg">
    </div>

    <div class="container">
        <div class="row">
            <div class="col-sm-12 text-center">
                <h1>@Strings.RegistryTitle</h1>
            
                <p>
                    @Strings.RegistryDetails
                </p>
            </div>
        </div>
             

        <div class="row">
            <div class="col-md-4 col-md-offset-4 col-sm-6 col-sm-offset-3 text-center">
                <a href="https://www.weddingshop.com/Buy/View/30090">
                    <img alt="The Wedding Shop" src="~/images/wedding-shop.jpg">
                </a>
            </div>
        </div>

    </div>
</section>
